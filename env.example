# Monarch Passport MVP - Environment Variables Example
# Copy this file to .env and fill in your actual values

# Supabase Configuration (REQUIRED)
REACT_APP_SUPABASE_URL=https://your-project-id.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your-anon-key-here

# Optional: Service Role Key (for admin scripts only - NEVER use in frontend)
# SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Monarch Passport Specific Configuration
REACT_APP_APP_NAME=Monarch Passport
REACT_APP_APP_VERSION=1.0.0

# QR Code Configuration
REACT_APP_QR_PREFIX=MONARCH
REACT_APP_QR_VERSION=1

# Security Configuration
REACT_APP_ENABLE_DEBUG=false
REACT_APP_ENABLE_ANALYTICS=false

# Feature Flags
REACT_APP_ENABLE_LIMITED_EDITIONS=true
REACT_APP_ENABLE_SEASONAL_STAMPS=true
REACT_APP_ENABLE_REFERRALS=true

# Development Configuration
REACT_APP_ENABLE_MOCK_DATA=false
REACT_APP_ENABLE_PERFORMANCE_MONITORING=false 